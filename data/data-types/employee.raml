#%RAML 1.0 DataType
---

type: object
properties:
  name: string
  gender:
    enum: [FEMALE, MALE]
  age: integer
  joinDate: date-only
  homePage: Url
  role:
    enum: [DEV, UX, DEVOPS, QA, BA, PM, SALES]
  grade:
    enum: [CON, JUNIOR, SENIOR, LEAD, PRINCIPLE]

example: !include ../examples/employee.json