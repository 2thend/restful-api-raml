#%RAML 1.0 DataType
---

type: object
properties:
  name: string
  description: string
  location: string
  createDate: date-only
  type:
    enum: ["民营企业", "外资企业", "合资企业", "事业单位", "国有企业"]
  scale:
    enum: ["<50", "500-1000", "500-1000", "1000-5000", ">5000"]
  employeeAmount: number
  officeAmount: number
  homePage: Url

example: !include ../examples/company.json
